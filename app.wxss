@import './assets/css/weui.wxss';

.weui-demo-dialog{
  visibility:hidden;
  opacity:0;
  transition:all .3s;
}

.weui-demo-dialog_show{
  visibility:visible;
  opacity:1;
}
.btn-cover {
  position: absolute;
  opacity: 0;
  top: 0;
  left: 0; 
  right: 0;
  bottom: 0;
  padding: 8px 200px !important; 
}
.layers{
  position:fixed;
  width: 100%;
  height: 100%;
  background-color: #666;
  opacity: 0.5;
  top: 0;
  left: 0;
  z-index: 2;
}